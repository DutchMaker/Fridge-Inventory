{
  "expo": {
    "name": "Fridge Inventory",
    "slug": "fridge-inventory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#108eef"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.nfc-registration"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-nfc-manager"
    ],
    "android": {
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.anonymous.nfcregistration"
    }
  }
}
